{
  "name": "@mirta/store",
  "description": "Type-safe store for wb-rules",
  "version": "0.1.2",
  "license": "Unlicense",
  "keywords": [
    "mirta",
    "store",
    "wb-rules"
  ],
  "type": "module",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "homepage": "https://github.com/wb-mirta/core/tree/latest/packages/mirta-store#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wb-mirta/core.git",
    "directory": "packages/mirta-store"
  },
  "bugs": {
    "url": "https://github.com/wb-mirta/core/issues"
  },
  "funding": {
    "type": "individual",
    "url": "https://pay.cloudtips.ru/p/58512cca"
  },
  "scripts": {
    "clean": "rimraf dist build",
    "build": "pnpm clean && rollup -c ../../rollup.config.mjs",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@mirta/polyfills": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}
